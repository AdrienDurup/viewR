{
  "name": "viewr-support-for-html",
  "version": "0.0.1",
  "publisher": "Durad",
  "engines": {
    "vscode": "^1.60.0"
},
"categories": [
    "Programming Languages"
],
  "contributes": {
      "languages": [{
        "id":"html",
          "extensions": ["html,vrmu"],
          "configuration": "./language-configuration.json"
      }],
      "grammars": [{
        "injectInto": "html.text.basic",
          "scopeName": "source.viewr",
          "path": "./syntaxes/viewr.injection.json"
      }]
  }
}